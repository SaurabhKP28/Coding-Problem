Find Largest Element
 
 
class Solution {

    public int findMaximum(int[] nums) {

        int maxi = nums[0];
        for(int i = 1; i < nums.length; i++)
        {
            if(nums[i] > maxi)
            {
                maxi = nums[i];
            }
        }

        return maxi;

    }

}
    
